		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>

		<footer-portlet-javascript>/plugins/Leaflet.label-0.2.1/dist/leaflet.label.js</footer-portlet-javascript>
		<footer-portlet-javascript>/plugins/leaflet-indoor-master/leaflet-indoor.js</footer-portlet-javascript>
		<footer-portlet-javascript>/plugins/Leaflet.Bookmarks-0.2.5/dist/Leaflet.Bookmarks.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/definitions_geoJSON.js</footer-portlet-javascript>
		
		<css-class-wrapper>politaktiv-map2-portlet</css-class-wrapper>	
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
