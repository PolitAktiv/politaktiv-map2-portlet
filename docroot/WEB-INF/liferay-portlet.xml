		<instanceable>true</instanceable>
		
		<header-portlet-css>/css/leaflet-basis.css</header-portlet-css>
		<header-portlet-css>/plugins/Leaflet.draw-0.2.4/dist/leaflet.draw.css</header-portlet-css>
		<header-portlet-css>/plugins/Leaflet.label-0.2.1/dist/leaflet.label.css</header-portlet-css>
		<header-portlet-css>/plugins/leaflet.viewcenter-master/leaflet.viewcenter.css</header-portlet-css>
		<header-portlet-css>/plugins/leaflet.control.orderlayers-0.2.0/leaflet.control.orderlayers.css</header-portlet-css>
		<header-portlet-css>/plugins/Leaflet.Bookmarks-0.2.5/dist/leaflet.bookmarks.css</header-portlet-css>
		<header-portlet-css>/css/main.css</header-portlet-css>
		
		<footer-portlet-javascript>/js/leaflet-basis.js</footer-portlet-javascript>
		<footer-portlet-javascript>/plugins/leaflet.viewcenter-master/leaflet.viewcenter.js</footer-portlet-javascript>
		<footer-portlet-javascript>/plugins/leaflet.control.orderlayers-0.2.0/leaflet.control.orderlayers.js</footer-portlet-javascript>
